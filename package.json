{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "sanity-project-management",
  "title": "Sanity",
  "description": "Interact with your Sanity projects",
  "icon": "command-icon.png",
  "author": "kmelve",
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Manage Projects",
      "description": "List and manage your Sanity projects",
      "mode": "view"
    },
    {
      "name": "createProject",
      "title": "Create Project",
      "description": "Create a Sanity project",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.48.8",
    "@raycast/utils": "^1.9.1",
    "@sanity/cli": "^3.6.0",
    "@sanity/client": "^5.2.2",
    "configstore": "^6.0.0",
    "node-fetch": "^3.3.2"
  },
  "devDependencies": {
    "@raycast/eslint-config": "1.0.5",
    "@types/configstore": "^6.0.0",
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "eslint": "^7.32.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "ray publish"
  }
}
